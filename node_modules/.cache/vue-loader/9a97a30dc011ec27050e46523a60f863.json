late>\n\n<script>\nimport firebase from 'firebase'\nexport default {\n  name: 'Signup',\n  data() {\n    return {\n      isLoad: false,\n      name: null,\n      email: null,\n      password: null\n    }\n  },\n  methods: {\n    Register() {\n      this.isLoad = true\n\n      firebase\n        .auth()\n        .createUserWithEmailAndPassword(this.email, this.password)\n        .then(user => {\n          console.log(user)\n          //this.$router.replace('/')\n        })\n        .catch(err => alert(err.message))\n\n      this.isLoad = false\n\n    }\n  }\n}\n</script>\n\n<style lang=\"css\" scoped>\n</style>\n"]}]}
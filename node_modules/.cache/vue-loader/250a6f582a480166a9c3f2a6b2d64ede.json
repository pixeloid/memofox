sLeftAsideExpanded = !this.hasLeftAsideExpanded\n    },\n    asideRightToggle() {\n      this.hasRightAsideExpanded = !this.hasRightAsideExpanded\n    },\n\n    logout() {\n      firebase.auth().signOut()\n        .then(() => console.log('Signed out'))\n        .catch(err => alert(err.message))\n    }\n\n  }\n}\n</script>\n\n<style lang=\"css\" scoped>\n</style>\n"]}]}
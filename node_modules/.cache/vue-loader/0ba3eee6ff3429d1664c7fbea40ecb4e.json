rcesContent":["<template>\n  <div class=\"columns mt-6\">\n    <div class=\"column is-three-fifths is-offset-one-fifth\">\n      <div class=\"card\">\n\n        <div class=\"card-header p-5\">\n          <h2 class=\"title\">Bejelentkezés</h2>\n        </div>\n\n        <div class=\"card-content\">\n          <form class=\"form\">\n\n\n            <div class=\"field\">\n              <label class=\"label\">Email</label>\n              <div class=\"control\">\n                <input class=\"input\" v-model=\"email\" type=\"email\" placeholder=\"e.g. alex@example.com\">\n              </div>\n            </div>\n\n            <div class=\"field\">\n              <label class=\"label\">Password</label>\n              <div class=\"control\">\n                <input class=\"input\" v-model=\"password\" type=\"password\" placeholder=\"********\">\n              </div>\n            </div>\n\n            <button class=\"button is-primary\" @click=\"Login\">Sign in</button>\n            Vagy <router-link :to=\"{ name: 'Signup', params: {} }\">Regisztrálj</router-link>!\n\n          </form>\n        </div>\n\n        <footer class=\"card-footer\">\n\n        </footer>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport firebase from 'firebase'\nexport default {\n  data() {\n    return {\n      isLoad: false,\n      email: null,\n      password: null\n    }\n  },\n  methods: {\n    Login() {\n      firebase\n        .auth()\n        .signInWithEmailAndPassword(this.email, this.password)\n        .then(data => console.log(data))\n        .catch(err => alert(err.message))\n    }\n  }\n}\n</script>\n\n<style lang=\"css\" scoped>\n</style>\n"]}]}